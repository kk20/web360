.include "m2560def.inc"
.def multiplication_low = r0		; define a to be register r16
.def multiplication_high = r1		; define b to be register r17
.def counter = r17
.def value = r18
.def higher_sum = r21;
.def lower_sum = r20;
.macro power

loop:	mul lower_sum, value;
	movw  r2, multiplication_low	
	mul higher_sum, value;
	add r3,r0;
	movw lower_sum, r2; 
	dec counter;
	brbc 1,loop;
.endmacro

.macro subtraction

.endmacro	


start:
	
	ldi counter, $04;
	;dec counter;
	ldi value, $02;
	add lower_sum, value;
	power 
	subtraction

end:
	rjmp end ; Halt the processor execution
    
